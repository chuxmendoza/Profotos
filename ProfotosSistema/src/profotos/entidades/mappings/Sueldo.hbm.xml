<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="profotos.entidades.clases.Sueldo" table="sueldos">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id> 
        <property name="sueldoLider" type="int" column="sueldoLider"/> 
        <property name="sueldoFotografo" type="int" column="sueldoFotografo"/> 
        <property name="sueldoVendedor" type="double" column="sueldoVendedor"/>  
    </class>
</hibernate-mapping>